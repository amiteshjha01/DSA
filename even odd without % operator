#include <iostream>
using namespace std;

string isEvenOrOdd(int number) {
    if ((number & 1) == 0) {
        return "Even";
    } else {
        return "Odd";
    }
}

int main() {
    int number;
    cout << "Enter a number: ";
    cin >> number;

    string result = isEvenOrOdd(number);
    cout << number << " is " << result << endl;

    return 0;
}
